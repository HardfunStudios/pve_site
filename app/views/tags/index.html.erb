<p id="notice"><%= notice %></p>

<h1>Tags</h1>

<table>
  <thead>
    <tr>
      <th>Term wp</th>
      <th>Name</th>
      <th>Slug</th>
      <th>Term group</th>
      <th>Term taxonomy</th>
      <th>Taxonomy</th>
      <th>Description</th>
      <th>Parent wp</th>
      <th>Count</th>
      <th>Filter</th>
      <th>Parent</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tags.each do |tag| %>
      <tr>
        <td><%= tag.term_wp_id %></td>
        <td><%= tag.name %></td>
        <td><%= tag.slug %></td>
        <td><%= tag.term_group %></td>
        <td><%= tag.term_taxonomy_id %></td>
        <td><%= tag.taxonomy %></td>
        <td><%= tag.description %></td>
        <td><%= tag.parent_wp_id %></td>
        <td><%= tag.count %></td>
        <td><%= tag.filter %></td>
        <td><%= tag.parent_id %></td>
        <td><%= link_to 'Show', tag %></td>
        <td><%= link_to 'Edit', edit_tag_path(tag) %></td>
        <td><%= link_to 'Destroy', tag, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tag', new_tag_path %>
